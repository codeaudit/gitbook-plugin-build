{
	"opts": {
		"destination": "./build/docs/changelog.md",
		"encoding": "utf8",
		"recurse": true
	},
	"tags": {
		"allowUnknownTags": false
	},
	"plugins": [
		"plugins/markdown",
		"config/jsdocPlugins/specs",
		"config/jsdocPlugins/tests"
	],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	}
}
